global k = 5

global values
global occurs

probe syscall.*.return {
    exec = execname()

    if (exec == "stap" || exec == "stapio" || exec == "python")
        next;

    values[exec,occurs[exec]] = name
    occurs[exec]++

    if (occurs[exec] == k) {
        printf("%s %d", exec, uid())

        for (i=0; i < k; i++) {
            printf(" %s", values[exec,i])
            values[exec,i] = values[exec,i+1]
        }

        printf("\n");
        occurs[exec]--
    }
}
